{
  "name": "floor12/yii2-module-articles",
  "description": "Articles and news with postgres full text search.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "news",
    "articles",
    "postresql",
    "postres"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "floor12@floor12.net",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "floor12/yii2-editmodal": "*",
    "floor12/yii2-module-pages": "*",
    "floor12/yii2-module-files": "*",
    "floor12/yii2-metamaster": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\articles\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\articles\\tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-metamaster"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    }
  ]
}
